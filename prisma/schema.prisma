generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id    Int    @id @default(autoincrement())
  name  String
  email String
}

model Tool {
  id          String @id @default(uuid())
  name        String @unique
  url         String
  description String
  tags        String
}
